    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0",
      "prettier --write"
